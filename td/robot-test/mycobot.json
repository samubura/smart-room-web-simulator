{
    "@context": "https://www.w3.org/2019/wot/td/v1",
    "@type": "MyCoBot",
    "id": "http://localhost:3000/workspaces/robot-test/things/mycobot",
    "title": "mycobot",
    "base": "http://localhost:3000/affordances/robot-test/mycobot/",
    "securityDefinitions": {
        "nosec_sc": {
            "scheme": "nosec"
        }
    },
    "security": [
        "nosec_sc"
    ],
    "properties": {
        "angles": {
            "type": "array",
            "items": {
                "type": "number",
                "minimum": -160,
                "maximum": 160
            },
            "minItems": 6,
            "maxItems": 6,
            "forms": [
                {
                    "href": "angles",
                    "op": [
                        "readproperty"
                    ],
                    "contentType": "application/json"
                }
            ],
            "readOnly": true
        },
        "coordinates": {
            "type": "object",
            "properties": {
                "X": {
                    "type": "number",
                    "minimum": -300,
                    "maximum": 300
                },
                "Y": {
                    "type": "number",
                    "minimum": -300,
                    "maximum": 300
                },
                "Z": {
                    "type": "number",
                    "minimum": -300,
                    "maximum": 300
                },
                "Rx": {
                    "type": "number",
                    "minimum": -180,
                    "maximum": 180
                },
                "Ry": {
                    "type": "number",
                    "minimum": -180,
                    "maximum": 180
                },
                "Rz": {
                    "type": "number",
                    "minimum": -180,
                    "maximum": 180
                }
            },
            "forms": [
                {
                    "href": "coordinates",
                    "op": [
                        "readproperty"
                    ],
                    "contentType": "application/json"
                }
            ],
            "readOnly": true
        },
        "headColor": {
            "type": "object",
            "properties": {
                "red": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                },
                "green": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                },
                "blue": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                }
            },
            "forms": [
                {
                    "href": "headColor",
                    "op": [
                        "readproperty"
                    ],
                    "contentType": "application/json"
                }
            ],
            "readOnly": true
        },
        "gripper": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4096,
            "forms": [
                {
                    "href": "gripper",
                    "op": [
                        "readproperty"
                    ],
                    "contentType": "application/json"
                }
            ],
            "readOnly": true
        }
    },
    "actions": {
        "writeAngles": {
            "input": {
                "type": "object",
                "properties": {
                    "jointAngles": {
                        "type": "array",
                        "items": {
                            "type": "number",
                            "minimum": -160,
                            "maximum": 160
                        },
                        "minItems": 6,
                        "maxItems": 6
                    },
                    "speed": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 100
                    }
                },
                "required": [
                    "jointAngles",
                    "speed"
                ]
            },
            "forms": [
                {
                    "href": "writeAngles",
                    "op": "invokeaction",
                    "contentType": "application/json"
                }
            ],
            "safe": false,
            "idempotent": true
        },
        "writeAngle": {
            "input": {
                "type": "object",
                "properties": {
                    "jointId": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 6
                    },
                    "angle": {
                        "type": "number",
                        "minimum": -160,
                        "maximum": 160
                    },
                    "speed": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 100
                    }
                },
                "required": [
                    "jointId",
                    "angle",
                    "speed"
                ]
            },
            "forms": [
                {
                    "href": "writeAngle",
                    "op": "invokeaction",
                    "contentType": "application/json"
                }
            ],
            "safe": false,
            "idempotent": true
        },
        "reachCoordinates": {
            "input": {
                "type": "object",
                "properties": {
                    "coordinates": {
                        "type": "object",
                        "properties": {
                            "X": {
                                "type": "number",
                                "minimum": -300,
                                "maximum": 300
                            },
                            "Y": {
                                "type": "number",
                                "minimum": -300,
                                "maximum": 300
                            },
                            "Z": {
                                "type": "number",
                                "minimum": -300,
                                "maximum": 300
                            },
                            "Rx": {
                                "type": "number",
                                "minimum": -180,
                                "maximum": 180
                            },
                            "Ry": {
                                "type": "number",
                                "minimum": -180,
                                "maximum": 180
                            },
                            "Rz": {
                                "type": "number",
                                "minimum": -180,
                                "maximum": 180
                            }
                        },
                        "required": [
                            "X",
                            "Y",
                            "Z",
                            "Rx",
                            "Ry",
                            "Rz"
                        ]
                    },
                    "speed": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 100
                    }
                },
                "required": [
                        "coordinates",
                        "speed"
                    ]
            },
            "forms": [
                {
                    "href": "reachCoordinates",
                    "op": [
                        "invokeaction"
                    ],
                    "contentType": "application/json"
                }
            ],
            "safe": false,
            "idempotent": true
        },
        "reachCoordinate": {
            "input": {
                "type": "object",
                "properties": {
                    "coordinateId": {
                        "type": "string",
                        "enum": [
                            "X",
                            "Y",
                            "Z",
                            "Rx",
                            "Ry",
                            "Rz"
                        ]
                    },
                    "value": {
                        "type": "number",
                        "minimum": -300,
                        "maximum": 300
                    },
                    "speed": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 100
                    }
                },
                "required": [
                        "coordinateId",
                        "value",
                        "speed"
                    ]
            },
            "forms": [
                {
                    "href": "reachCoordinate",
                    "op": [
                        "invokeaction"
                    ],
                    "contentType": "application/json"
                }
            ],
            "safe": false,
            "idempotent": true
        },
        "setColor": {
            "input": {
                "type": "object",
                "properties": {
                    "red": {
                        "type": "integer",
                        "maximum": 255,
                        "minimum": 0
                    },
                    "green": {
                        "type": "integer",
                        "maximum": 255,
                        "minimum": 0
                    },
                    "blue": {
                        "type": "integer",
                        "maximum": 255,
                        "minimum": 0
                    }
                },
                "required": [
                    "red",
                    "green",
                    "blue"
                ]
            },
            "forms": [
                {
                    "href": "setColor",
                    "op": "invokeaction",
                    "contentType": "application/json"
                }
            ],
            "safe": false,
            "idempotent": true
        },
        "setGripper": {
            "input": {
                "type": "object",
                "properties": {
                    "value": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 4096
                    },
                    "speed": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 100
                    }
                },
                "required": [
                    "value",
                    "speed"
                ]
            },
            "forms": [
                {
                    "href": "setGripper",
                    "op": "invokeaction",
                    "contentType": "application/json"
                }
            ],
            "safe": false,
            "idempotent": true
        }
    }
}